    "@types/jest": "24.0.13",
    "@types/nanoevents": "1.0.0",
    "@types/nanoid": "2.0.0",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-test-renderer": "16.8.1",
    "@types/styled-components": "4.1.15",
    "coveralls": "3.0.3",
    "jest": "24.8.0",
    "npm-run-all": "4.1.5",
    "react": "16.8.6",
    "react-test-renderer": "16.8.6",
    "styled-components": "4.2.1",
    "ts-jest": "24.0.2",
    "typedoc": "0.14.2",
