    "@types/nanoevents": "1.0.0",
    "@types/nanoid": "2.1.0",
    "@types/react": "16.14.15",
    "@types/react-dom": "17.0.9",
    "@types/react-test-renderer": "16.9.5",
    "@types/styled-components": "4.4.3",
    "coveralls": "3.1.1",
    "jest": "24.9.0",
    "jest-dom": "3.5.0",
    "npm-run-all": "4.1.5",
    "react": "16.8.6",
    "react-test-renderer": "16.8.6",
    "styled-components": "4.4.1",
    "ts-jest": "24.3.0",
    "typedoc": "0.20.36",
