/colors';\nimport CloseIcon from '../../../components/close-icon';\n\nexport const mixin = css`\n  display: flex;\n  align-items: center;\n  font-size: .8em;\n  color: '${SUMI}';\n  background: ${SHIRONERI};\n  border: 1px solid ${SHIRONEZUMI};\n  padding: 0.25em 0.5em;\n  margin-top: 0.3em;\n\n  .middle {\n    margin: 0 0.5em;\n  }\n\n  .right {\n    svg {\n      height: 0.7em;\n      opacity: 0.7;\n      transition: 0.2s;\n      transition-property: height, opacity;\n    }\n    svg:hover {\n      height: 0.8em;\n      opacity: 1;\n      cursor: pointer;\n    }\n  }\n`;\n\nconst Alert = styled.div`\n  ${mixin}\n`;\n\n// eslint-disable-next-line react/prop-types\nconst LogAlert: React.FC<BlockComponentProps> = (props): JSX.Element => {\n  return (\n    <Alert>\n      <div className=\"middle\">{props.children}</div>\n      <div className=\"right\">\n        <CloseIcon onClick={props.close} />\n      </div>\n    </Alert>\n  );\n};\n\nexport default SeedFactory.create({key: 'log', timeout: 5000}, LogAlert);\n"},"lineCount":null}},"error":null,"hash":"70d6ee997fb2f2dbcc0842353e418f29","cacheData":{"env":{}}}