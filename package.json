{
  "private": true,
  "description": "A React's notification component",
  "repository": "nju33/react-dayo",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "lerna bootstrap",
    "test": "lerna run test --stream --no-private",
    "rm-dist": "lerna exec --no-private -- rm -rf dist",
    "dev": "lerna run dev --stream --scope @example/react-dayo",
    "build": "lerna run build --stream --no-private",
    "publish": "lerna publish",
    "postpublish": "run-s push:*",
    "push:example": "gh-pages --dist examples/react-dayo --src '{pages/**,*}' --branch @example/react-dayo --dotfiles",
    "push:docs": "gh-pages --dist packages/react-dayo --src '{src/**,*}' --branch docs --dotfiles"
  },
  "devDependencies": {
    "@nju33/eslint-config": "0.0.26",
    "@nju33/prettier": "0.0.26",
    "eslint": "5.16.0",
    "gh-pages": "2.0.1",
    "lerna": "3.15.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "renovate-config-nju33": "0.0.1",
    "typescript": "3.5.1"
  },
  "dependencies": {}
}
